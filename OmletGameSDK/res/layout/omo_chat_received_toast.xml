<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:gravity="center"
    android:background="@drawable/omo_snackbar_small_background"
    android:layout_width="@dimen/omp_notification_width"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:layout_height="wrap_content">

    <TextView android:id="@+id/feed_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="9dp"
        android:textColor="#00c89b"
        android:maxWidth="68dp"
        android:textSize="10dp"
        android:background="@drawable/oml_feed_name_toast_box"
        android:ellipsize="end"
        android:singleLine="true"/>

    <TextView android:id="@+id/sender_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="75dp"
        android:layout_marginRight="7dp"
        android:textStyle="bold"
        android:textSize="12sp"
        android:textColor="#2eacd4"
        android:ellipsize="end"
        android:singleLine="true"/>

    <TextView android:id="@+id/sent_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:textColor="@android:color/white"
        android:singleLine="true"
        android:ellipsize="end"/>

</LinearLayout><!-- From: file:/Users/tj/Developer/omlib/glreclib/src/main/res/layout/omo_chat_received_toast.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/glreclib/unspecified/genericRelease/res/layout/omo_chat_received_toast.xml -->