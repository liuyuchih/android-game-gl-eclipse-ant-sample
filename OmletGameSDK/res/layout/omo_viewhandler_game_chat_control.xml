<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="38dip"
        android:layout_height="wrap_content"
        android:background="@drawable/oml_background_game_chat_control">
        <ImageButton
            android:id="@+id/image_button_quitchat"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="1dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/oml_chattabview_quitchat_button"/>

        <ImageButton
            android:id="@+id/image_button_contacts"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_below="@id/image_button_quitchat"
            android:layout_marginTop="2dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/oml_chattabview_contacts_button"/>

        <ImageButton
            android:id="@+id/image_button_minimize"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_below="@id/image_button_contacts"
            android:visibility="gone"
            android:layout_marginTop="1dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/oml_chattabview_minimize_button"/>
    </RelativeLayout>

