<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="56dp"
    android:layout_height="46dp">
    <mobisocial.omlib.ui.view.ProfileImageView
        style="@style/image_profile_picture"
        android:layout_marginLeft="10dp"/>
    <View
        style="@style/view_user_online"></View>
    <ImageView
        android:id="@+id/image_view_cancel"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_alignBottom="@id/image_profile_picture"
        android:layout_alignRight="@id/image_profile_picture"
        android:layout_marginRight="-3dp"
        android:layout_marginBottom="-3dp"
        android:src="@drawable/oml_btn_addmembertochat_cancel"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/view_group_mask"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/oml_view_add_member_item_background"
        android:visibility="invisible">
        <TextView
            android:id="@+id/text_more_count"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:textColor="@android:color/white"
            android:textSize="20sp"
            android:gravity="center"/>
    </RelativeLayout>
