<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">
    <LinearLayout
        android:id="@+id/view_group_top_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            style="@style/view_group_top_bar">
            <ImageButton
                style="@style/image_button_top_bar_back"/>
            <LinearLayout
                style="@style/view_group_top_bar_menu">
                <ImageButton
                    android:id="@+id/image_button_addfriend"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/oml_btn_contacts_addfriend"
                    android:background="@null"/>
            </LinearLayout>
            <TextView
                style="@style/text_top_bar_title"
                android:text="@string/friends"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/view_group_own_profile"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="@drawable/oml_clickable_white_background">
            <mobisocial.omlib.ui.view.ProfileImageView
                style="@style/image_profile_picture"
                android:layout_marginLeft="12dp"/>
            <TextView android:id="@+id/chat_member_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16sp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_centerVertical="true"
                android:textColor="@color/oml_members_name"
                android:layout_toRightOf="@id/image_profile_picture"
                android:singleLine="true"/>
            <View
                style="@style/view_line"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="0dp"
                android:layout_alignParentBottom="true"></View>
        </RelativeLayout>
    </LinearLayout>
    <mobisocial.omlib.ui.view.RecyclerView
        android:id="@+id/contact_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/view_group_top_profile"
        android:layout_alignParentBottom="true"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/view_group_empty_contact"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/view_group_top_profile"
        android:layout_alignParentBottom="true">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="vertical"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:textColor="@color/oml_time_text"
                android:text="@string/have_no_friends"
                android:gravity="center"/>
            <Button
                android:id="@+id/button_addfriend"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:layout_marginTop="18dp"
                android:background="@drawable/oml_btn_add_friends_background"
                android:text="@string/add_friends"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:textAllCaps="false"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout><!-- From: file:/Users/tj/Developer/omlib/omlib-ui/src/main/res/layout/oml_fragment_contact_list.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/omlib-ui/unspecified/res/layout/oml_fragment_contact_list.xml -->