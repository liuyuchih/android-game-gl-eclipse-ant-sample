<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="66dp"
    android:background="@android:color/transparent">
    <View
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginLeft="54dp"
        android:layout_centerVertical="true"
        android:background="@drawable/oml_clickable_white_background"></View>

    <mobisocial.omlib.ui.view.ProfileImageView
        android:id="@+id/picture"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginLeft="28dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"/>

    <TextView
        android:id="@+id/unread_count"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:gravity="center"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:textColor="@color/oml_action_text"
        android:textSize="10sp"
        android:layout_marginLeft="60dp"
        android:background="@drawable/oml_unread_count_background"
        android:visibility="gone"/>

    <RelativeLayout
        android:id="@+id/view_group_message_time"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="14dp"
        android:layout_marginRight="6dp"
        android:layout_alignParentRight="true">
        <TextView
            android:id="@+id/last_message_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/oml_time_text"
            android:textSize="12sp"
            android:gravity="right"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_toRightOf="@id/picture"
        android:layout_toLeftOf="@id/view_group_message_time"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/feed_name_and_kind"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="18sp"
                android:textColor="@color/oml_group_name_text"
                android:singleLine="true"
                android:ellipsize="end"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="4dp"
                android:gravity="center_vertical"
                android:background="@drawable/oml_view_group_number_background">
                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="4dp"
                    android:src="@drawable/oml_ic_glist_groupnumber"/>
                <TextView
                    android:id="@+id/text_groupnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="6dp"
                    android:textColor="@android:color/white"
                    android:textSize="12sp"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/feed_last_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/oml_time_text"
            android:singleLine="true"/>
    </LinearLayout>

</RelativeLayout><!-- From: file:/Users/tj/Developer/omlib/omlib-ui/src/main/res/layout/oml_feed_view_item.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/omlib-ui/unspecified/res/layout/oml_feed_view_item.xml -->