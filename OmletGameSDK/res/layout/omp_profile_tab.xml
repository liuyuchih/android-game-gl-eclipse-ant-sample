<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_line_eaeaea"></View>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/omp_gray_gradient"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal">
        <LinearLayout
            android:id="@+id/user_profile_upload"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/upload_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/omp_zero"
                    android:textColor="@color/omlet_blue"
                    android:textSize="14sp"/>
                <TextView
                    android:id="@+id/upload_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:text="@string/omp_upload"
                    android:textColor="@color/omlet_blue"
                    android:textSize="14sp"/>
            </LinearLayout>
            <View
                android:id="@+id/upload_line"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="@color/omlet_blue"
                android:visibility="visible"></View>
        </LinearLayout>
        <View
            android:layout_width="1dip"
            android:layout_height="22dip"
            android:layout_marginBottom="6dp"
            android:layout_gravity="bottom"
            android:background="@color/gray_line_eaeaea">
        </View>
        <LinearLayout
            android:id="@+id/user_profile_fans"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/fans_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/omp_zero"
                    android:textColor="@color/gray_text_70706f"
                    android:textSize="14sp"/>
                <TextView
                    android:id="@+id/fans_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:text="@string/omp_profile_tab_followers"
                    android:textColor="@color/gray_text_70706f"
                    android:textSize="14sp"/>
            </LinearLayout>
            <View
                android:id="@+id/fans_line"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="@color/omlet_blue"
                android:visibility="invisible"></View>
        </LinearLayout>
        <View
            android:layout_width="1dip"
            android:layout_height="22dip"
            android:layout_marginBottom="6dp"
            android:layout_gravity="bottom"
            android:background="@color/gray_line_eaeaea">
        </View>
        <LinearLayout
            android:id="@+id/user_profile_follow"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/follow_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/omp_zero"
                    android:textColor="@color/gray_text_70706f"
                    android:textSize="14sp"/>
                <TextView
                    android:id="@+id/follow_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:text="@string/omp_profile_tab_following"
                    android:textColor="@color/gray_text_70706f"
                    android:textSize="14sp"/>
            </LinearLayout>
            <View
                android:id="@+id/follow_line"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="@color/omlet_blue"
                android:visibility="invisible"></View>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="5dip"
        android:background="@color/gray_line_eaeaea"></View>
</merge><!-- From: file:/Users/tj/Developer/omlib/glreclib/src/main/res/layout/omp_profile_tab.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/glreclib/unspecified/genericRelease/res/layout/omp_profile_tab.xml -->