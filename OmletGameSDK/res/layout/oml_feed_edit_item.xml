<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="66dp"
    android:background="@android:color/transparent">
    <View
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginLeft="54dp"
        android:layout_centerVertical="true"
        android:background="@drawable/oml_clickable_white_background"></View>

    <mobisocial.omlib.ui.view.ProfileImageView
        android:id="@+id/picture"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginLeft="28dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"/>

    <RelativeLayout
        android:id="@+id/view_group_message_time"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginRight="6dp"
        android:layout_alignParentRight="true">
        <Button
            android:id="@+id/button_delete"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="6dp"
            android:layout_marginLeft="36dp"
            android:background="@drawable/oml_btn_group_delete_background"
            android:text="@string/delete"
            android:textColor="@android:color/white"
            android:textSize="16sp"
            android:textAllCaps="false"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_toRightOf="@id/picture"
        android:layout_toLeftOf="@id/view_group_message_time"
        android:layout_marginLeft="13dp"
        android:gravity="center_vertical">
        <TextView
            android:id="@+id/feed_name_and_kind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="@color/oml_group_name_text"
            android:singleLine="true"
            android:ellipsize="end"/>
    </LinearLayout>

</RelativeLayout><!-- From: file:/Users/tj/Developer/omlib/omlib-ui/src/main/res/layout/oml_feed_edit_item.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/omlib-ui/unspecified/res/layout/oml_feed_edit_item.xml -->