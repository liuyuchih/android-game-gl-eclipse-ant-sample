<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context="mobisocial.omlet.overlaybar.ui.fragment.ScreenshotDetailsFragment">
    <RelativeLayout
        style="@style/main_overlay_top_bar_container"
        android:id="@+id/relative_layout_top_bar">
        <ImageButton
            android:id="@+id/image_button_back"
            style="@style/video_overlay_back_button" />
        <ImageButton
            android:layout_width="40dip"
            android:layout_height="40dip"
            android:id="@+id/more"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="28.5dip"
            android:background="@drawable/omp_video_details_more_image_selector"/>
        <ImageButton
            android:layout_width="40dip"
            android:layout_height="40dip"
            android:id="@+id/share"
            android:layout_toLeftOf="@+id/more"
            android:layout_centerVertical="true"
            android:layout_marginRight="10.5dip"
            android:background="@drawable/omp_video_details_share_image_selector"/>
        <ImageButton
            android:layout_width="40dip"
            android:layout_height="40dip"
            android:id="@+id/like"
            android:layout_toLeftOf="@+id/share"
            android:layout_centerVertical="true"
            android:layout_marginRight="10.5dip"
            android:background="@drawable/omp_btn_onepage_like_normal"/>
        <TextView
            android:id="@+id/top_bar_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_toLeftOf="@+id/like"
            android:layout_toRightOf="@+id/image_button_back"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:textColor="@android:color/white"
            android:textSize="18sp"
            android:gravity="left"
            android:singleLine="true"/>
    </RelativeLayout>
    <ScrollView
        android:id="@+id/scrollview_content"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/relative_layout_top_bar"
        android:background="@android:color/white">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/view_group_scroll_view_content_container">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:id="@+id/view_group_video_info"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/image_view_game"/>
                <RelativeLayout
                    android:id="@+id/video_btn_view_group"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true">
                    <TextView
                        style="@style/button_background_omlet_blue_rounded_corner"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="@string/omp_video_details_download"
                        android:id="@+id/text_view_download_button"
                        android:visibility="gone"/>
                    <ToggleButton android:id="@+id/follow_button"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:minWidth="94dp"
                        android:gravity="center"
                        android:paddingLeft="18dp"
                        android:paddingRight="18dp"
                        android:textOff="@string/omp_follow_button"
                        android:textOn="@string/omp_following"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:checked="false"
                        android:background="@drawable/omp_profile_follow_button"
                        android:textColor="@drawable/omp_profile_follow_text_color"
                        android:drawablePadding="3dp"
                        android:drawableLeft="@drawable/omp_profile_follow_drawable" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/image_view_game"
                    android:layout_toLeftOf="@id/video_btn_view_group"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingLeft="6dp"
                        android:paddingRight="6dp"
                        android:id="@+id/linear_layout_details_container"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/text_view_game_name"
                            android:text="@string/omp_stub_user_id_or_game_name"
                            android:textColor="@color/gray_text_4a4a4a"
                            android:textSize="16sp"
                            android:maxLines="1"
                            android:singleLine="true"
                            android:ellipsize="end"/>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:paddingRight="2dp"
                                android:id="@+id/text_view_user_id"
                                android:textColor="@color/gray_text_4a4a4a"
                                android:textSize="11sp"
                                android:text="@string/omp_stub_user_id"
                                android:singleLine="true"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/text_view_post_time"
                                android:textColor="@color/gray_text_4a4a4a"
                                android:textSize="9sp"
                                android:text="@string/omp_stub_time_label"
                                android:layout_toRightOf="@id/text_view_user_id"
                                android:singleLine="true"
                                android:ellipsize="none"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="174dp"
                android:id="@+id/screenshot_preview"
                android:layout_below="@id/view_group_video_info"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/omp_btn_omplaylogo_loading"
                android:background="@color/gray_background_bcbcbc"
                android:scaleType="center"/>

        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical"
                      android:layout_below="@id/screenshot_preview" android:id="@+id/view_group_video_post_info">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingTop="12dp"
                android:paddingBottom="12dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp">
                <LinearLayout android:id="@+id/edit_title_area" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/edit_title_button" android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/omp_btn_editname_myprofile"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginTop="6dp"
                        android:id="@+id/text_view_video_title"
                        android:hint="@string/omp_edit_title"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:textColor="@android:color/black"/>
                </LinearLayout>

                <LinearLayout android:id="@+id/edit_description_area" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/edit_description_button" android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/omp_btn_editname_myprofile"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/text_view_video_description"
                        android:hint="@string/omp_edit_description"
                        android:layout_marginTop="6dp"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:textSize="11sp"
                        android:textColor="@color/gray_text_4a4a4a"/>
                </LinearLayout>
                <RelativeLayout
                        android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view_group_tag_container"
                    android:layout_marginTop="6dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:paddingRight="8dp">
                    <mobisocial.omlib.ui.view.FlowLayout
                        android:id="@+id/tag_flowlayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:minHeight="18dp"
                        app:verticalSpacing="8dp">
                    </mobisocial.omlib.ui.view.FlowLayout>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view_group_likes_and_views"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="12dp"
                    android:gravity="center_vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/text_view_like_count"
                        android:text="@string/omp_zero"
                        android:textSize="9sp"
                        android:textColor="@color/gray_text_585857"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="@string/omp_Likes"
                        android:textSize="9sp"
                        android:textColor="@color/gray_text_585857"/>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="10dp"
                        android:background="@color/gray_line_b6b6b6"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:id="@+id/view_separator_likes_views"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/text_view_view_count"
                        android:text="@string/omp_zero"
                        android:textSize="9sp"
                        android:textColor="@color/gray_text_585857"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="@string/omp_Views"
                        android:textSize="9sp"
                        android:textColor="@color/gray_text_585857"/>
                </LinearLayout>
                </LinearLayout>
                <LinearLayout android:layout_width="80dp" android:layout_height="wrap_content" android:orientation="vertical" android:padding="5dp"
                              android:layout_marginTop="12dp"
                              android:layout_marginRight="10dp">
                    <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/image_view_game_icon"/>
                    <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingRight="2dp"
                            android:layout_marginTop="4dp"
                            android:ellipsize="none"
                            android:scrollHorizontally="false"
                            android:maxLines="5"
                            android:id="@+id/text_view_game_app_name"
                            android:textColor="@color/gray_text_4a4a4a"
                            android:textSize="11sp"
                            android:text="@string/omp_stub_game_name"
                            android:gravity="center"
                            android:singleLine="false"/>
                </LinearLayout>
            </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <ImageView
        android:id="@+id/scrolldownhint"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/omp_btn_scrolldownhint_onepage"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="6dp"/>
    <View
        android:id="@+id/more_background"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/relative_layout_top_bar"
        android:layout_alignParentBottom="true"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/view_group_more_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginRight="35dp"
        android:layout_alignParentRight="true">
    </RelativeLayout>
</RelativeLayout>
