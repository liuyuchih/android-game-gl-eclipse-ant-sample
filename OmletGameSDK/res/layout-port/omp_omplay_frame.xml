<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="11dip"
        android:layout_marginBottom="11dip"
        android:layout_marginLeft="11dip"
        android:layout_marginRight="11dip"
        android:background="@drawable/omp_omplay_frame"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginLeft="15dip"
        android:layout_marginRight="15dip"
        android:layout_marginBottom="15dip"
        android:layout_alignParentBottom="true"
        android:background="@color/omp_background"/>
    <ScrollView
        android:id="@+id/navigation_bar"
        android:layout_width="match_parent"
        android:layout_height="66dip"
        android:layout_marginLeft="15dip"
        android:layout_marginRight="15dip"
        android:layout_marginBottom="10dip"
        android:layout_alignParentBottom="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="66dip"
            android:orientation="horizontal"
            android:gravity="center_vertical">

            <LinearLayout
                android:id="@+id/button0"
                android:layout_width="56dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dip"
                android:gravity="center"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="40dip"
                    android:layout_height="40dip"
                    android:background="@drawable/omp_btn_bottomtab_omletlogo"/>
            </LinearLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="66dip"
                android:layout_weight="1">

                <RelativeLayout
                    android:id="@+id/button1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="56dip"
                        android:layout_height="56dip"
                        android:layout_centerInParent="true"
                        android:background="@drawable/omp_ic_maintab_latest_normal"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/omp_popular"
                        android:gravity="center"
                        android:textColor="#b2ffffff"
                        android:textSize="9sp"
                        android:layout_marginTop="41dip"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/button1_active_overlay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/omp_navigation_button_highlight"
                    android:visibility="gone">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="56dip"
                            android:layout_height="56dip"
                            android:layout_centerInParent="true"
                            android:background="@drawable/omp_ic_maintab_latest_click"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/omp_popular"
                            android:gravity="center"
                            android:textColor="@color/omp_background"
                            android:textSize="9sp"
                            android:layout_marginTop="41dip"/>
                    </RelativeLayout>
                </RelativeLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="65dip"
                android:layout_weight="1">
                <RelativeLayout
                    android:id="@+id/button3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="56dip"
                        android:layout_height="56dip"
                        android:layout_centerInParent="true"
                        android:background="@drawable/omp_ic_maintab_follow_normal"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/omp_following"
                        android:gravity="center"
                        android:textColor="#b2ffffff"
                        android:textSize="9sp"
                        android:layout_marginTop="41dip"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/button3_active_overlay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/omp_navigation_button_highlight"
                    android:visibility="gone">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="56dip"
                            android:layout_height="56dip"
                            android:layout_centerInParent="true"
                            android:background="@drawable/omp_ic_maintab_follow_click"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/omp_following"
                            android:gravity="center"
                            android:textColor="@color/omp_background"
                            android:textSize="9sp"
                            android:layout_marginTop="41dip"/>
                    </RelativeLayout>
                </RelativeLayout>
            </FrameLayout>


            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="65dip"
                android:layout_weight="1">
                <RelativeLayout
                    android:id="@+id/button4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="56dip"
                        android:layout_height="56dip"
                        android:layout_centerInParent="true"
                        android:background="@drawable/omp_ic_maintab_profile_normal"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/omp_profile"
                        android:gravity="center"
                        android:textColor="#b2ffffff"
                        android:textSize="9sp"
                        android:layout_marginTop="41dip"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/button4_active_overlay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/omp_navigation_button_highlight"
                    android:visibility="gone">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="56dip"
                            android:layout_height="56dip"
                            android:layout_centerInParent="true"
                            android:background="@drawable/omp_ic_maintab_profile_click"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/omp_profile"
                            android:gravity="center"
                            android:textColor="#2eabd4"
                            android:textSize="9sp"
                            android:layout_marginTop="41dip"/>
                    </RelativeLayout>
                </RelativeLayout>
            </FrameLayout>


            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="65dip"
                android:layout_weight="1">
                <RelativeLayout
                    android:id="@+id/button5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="56dip"
                        android:layout_height="56dip"
                        android:layout_centerInParent="true"
                        android:background="@drawable/omp_ic_maintab_notification_normal"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/omp_notifications"
                        android:gravity="center"
                        android:textColor="#b2ffffff"
                        android:textSize="9sp"
                        android:layout_marginTop="41dip"/>
                    <TextView
                        android:id="@+id/noti_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#ffffff"
                        android:textSize="9sp"
                        android:layout_marginTop="13dip"
                        android:layout_marginRight="14dip"
                        android:layout_alignParentRight="true"
                        android:paddingTop="1dip"
                        android:paddingBottom="1dip"
                        android:paddingLeft="4dip"
                        android:paddingRight="4dip"
                        android:background="@drawable/omp_white_border_red_fill_round_background"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/button5_active_overlay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/omp_navigation_button_highlight"
                    android:visibility="gone">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="56dip"
                            android:layout_height="56dip"
                            android:layout_centerInParent="true"
                            android:background="@drawable/omp_ic_maintab_notification_click"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/omp_notifications"
                            android:gravity="center"
                            android:textColor="@color/omp_background"
                            android:textSize="9sp"
                            android:layout_marginTop="41dip"/>
                    </RelativeLayout>
                </RelativeLayout>
            </FrameLayout>

        </LinearLayout>
    </ScrollView>
    <!--
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="15dip"
        android:layout_marginBottom="15dip"
        android:layout_marginLeft="71dip"
        android:layout_marginRight="15dip"
        android:orientation="vertical">
    -->
    <RelativeLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="15dip"
        android:layout_above="@id/navigation_bar"
        android:layout_marginLeft="15dip"
        android:layout_marginRight="15dip">
    </RelativeLayout>
    <!--
    </LinearLayout>
    -->
    
    <RelativeLayout
        android:id="@+id/view_group_tutorial"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
        <LinearLayout
            android:id="@+id/view_group_tutorial_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:paddingLeft="11dp"
                android:paddingRight="11dp"
                android:text="@string/omp_tutorial_set_profile_information"
                android:textColor="@android:color/white"
                android:textSize="15sp"
                android:gravity="center"
                android:background="@drawable/omp_tutorial_text_background"/>
            <ImageView
                android:layout_width="10dp"
                android:layout_height="8dp"
                android:layout_marginTop="-1dp"
                android:layout_marginRight="23dp"
                android:layout_gravity="right"
                android:background="@drawable/omp_tutorialbubble_triangledown"/>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/close_frame"
        android:layout_width="42dip"
        android:layout_height="42dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:gravity="center">
        <ImageView
            android:layout_width="38dip"
            android:layout_height="38dip"
            android:background="@drawable/omp_btn_clospeage"/>
    </RelativeLayout>

</RelativeLayout><!-- From: file:/Users/tj/Developer/omlib/glreclib/src/main/res/layout-port/omp_omplay_frame.xml --><!-- From: file:/Users/tj/Developer/omlib/glreclib-shell/build/intermediates/exploded-aar/omlib/glreclib/unspecified/genericRelease/res/layout-port/omp_omplay_frame.xml -->